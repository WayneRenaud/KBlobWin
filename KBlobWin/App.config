<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <add key="speed" value="60"/> <!-- FPS -->
      <add key="brightness" value = "12"/> <!-- Redness per step, default 12, max 255 -->
      <add key="screensaver" value = "0"/> <!-- Which screensaver to start -->
                                           <!-- 0 - Random Linear -->
                                           <!-- 1 - Horizontal Sine -->
                                           <!-- 2 - Circular Bounce -->
                                           <!-- 3 - Polar Coordinates -->
                                           <!-- 999 - Random -->
      <add key="cycletime" value = "120"/> <!-- Seconds before restarting. Default 120. -->
      <add key="humps" value = "7"/><!-- Max number of sine waves during the horizontal sine saver. -->
      <add key="boxesperframe" value="1"/><!-- Number of times to update the screensaver between frames. Default 1. -->
    </appSettings>
</configuration>